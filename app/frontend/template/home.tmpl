{{template "header" .}}
<div class="row">
    {{range .Items}}
    <div class="card col-xl-3 col-lg-4 col-md-6 col-sm-12 p-5 border-0">
        <img alt="..." class="card-img-top" src="{{ .Picture}}">
        <div class="card-body">
            <p class="card-text">{{ .Name}}</p>
            <h5 class="card-title">{{ .Price}}</h5>
        </div>
    </div>
    {{end}}

</div>
{{template "footer"}}